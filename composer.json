{
    "name": "frnkly/laravel-embeds",
    "type": "library",
    "description": "API embeds for Laravel",
    "keywords": ["embed", "api", "laravel"],
    "homepage": "http://github.com/frnkly/laravel-embeds",
    "support": {
        "issues": "https://github.com/frnkly/laravel-embeds/issues",
        "source": "https://github.com/frnkly/laravel-embeds"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Francis Amankrah",
            "email": "frank@frnk.ca",
            "homepage": "http://frnk.ca"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Frnkly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frnkly\\Spec\\": "spec/"
        }
    },
    "require-dev": {
        "crysalead/kahlan": "^2.5",
        "laravel/framework": "^5.2"
    },
    "config": {
        "bin-dir": "bin"
    }
}
